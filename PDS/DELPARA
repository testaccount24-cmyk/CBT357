/**  REXX -- DELETE DEBUGGING PARAGRAPH DISPLAY'S  **/
   SUBCOM ISPEXEC
   IF RC <> 0 THEN DO
      SAY 'DELPARA EDIT MACRO CAN ONLY BE RUN UNDER ISPF EDIT!'
      RETURN
   END
   SUBCOM ISREDIT
   IF RC = 0 THEN DO
      ADDRESS ISREDIT "ISREDIT MACRO (PARMS) NOPROCESS"
      PARMS = TRANSLATE(PARMS)
   END
   ELSE DO
      SAY 'DELPARA EDIT MACRO CAN ONLY BE RUN UNDER ISPF EDIT!'
      RETURN
   END

   "ISREDIT X ALL"
   "ISREDIT FIND ALL '~DEBUG~'"
   "ISREDIT DEL ALL NX"
   "ISREDIT RESET"

