//-YOUR-USERID-C JOB  (-YOUR-ACCT-INFO-),'-YOUR-NAME-',
//             CLASS=2,MSGCLASS=X,NOTIFY=-YOUR-USERID-
//***
//***  UNPACK DATA INTO SAME COLUMNS CHECKING FOR OVERFLOW
//***
//CLEANUP    EXEC     PGM=IEFBR14
//DEL01        DD       DSN=...,
//             DISP=(MOD,DELETE,DELETE),UNIT=SYSDA,SPACE=(TRK,0)
//***
//EZ         EXEC     PGM=EZTPA00,REGION=8M
//EZTVFM       DD       UNIT=SYSDA,SPACE=(CYL,(20,20))
//SYSOUT       DD       SYSOUT=*
//SYSPRINT     DD       SYSOUT=*
//INFILE       DD       DISP=SHR,DSN=...
//OUTFILE      DD       DSN=...,
//             DISP=(,CATLG),UNIT=SYSDA,SPACE=(CYL,(05,02),RLSE)
*
FILE I
     DATA        1 120  A
     PKED       43  10  P
*
FILE O        FB(00120 27960)
     DATA        1 120  A
     UNPKED     43  10  N
*
FILE OVFLOW   PRINTER
*
JOB  INPUT  I
     O:DATA = I:DATA
     UNPKED = PKED
     IF PKED > 9999999999
        DISPLAY OVFLOW '**  PROBLEM - RECORD ' RECORD-COUNT +
           ' IS BIGGER THAN THE RECEIVING FIELD (' +
           PKED ')'
     END-IF
     PUT O
*
//OVFLOW       DD       SYSOUT=*
//***
